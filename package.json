{
  "name": "todo_app",
  "version": "1.0.0",
  "description": "todo app made with electron",
  "main": "public/electron.js",
  "scripts": {
    "electron:start": "wait-on http://localhost:3000 && cross-env NODE_ENV=production electron .",
    "react:start": "react-scripts start",
    "start": "concurrently \"npm run react:start\" \"npm run electron:start\"",
    "react:build": "react-scripts build",
    "build": "npm run react:build && electron-builder --publish=always",
    "dev": "cross-env NODE_ENV=development nodemon --exec electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NohJanghan/todo_app.git"
  },
  "author": "Janghan Noh",
  "license": "",
  "bugs": {
    "url": "https://github.com/NohJanghan/todo_app/issues"
  },
  "homepage": "./",
  "build": {
    "appId": "com.example.electron-app",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.19",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@types/dotenv": "^8.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron": "^23.0.0",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^2.0.22",
    "react-scripts": "^5.0.1",
    "ts-loader": "^9.4.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
